	<value id="admin">Administration</value>
	<value id="iPhone">iPhone</value>
    </group>
    <!-- INIT DEFAULT CHANNEL				-->
    <group name="CHANNEL_normal" modifRight="2">
	<!-- cookie name for this channel		-->
	<value id="name">ZUNOSXA</value>
	<!-- directory where script page are stored	-->
	<value id="path"/>
	<!-- private channel need a user session to view-->
		<!-- the channel content If you have both public-->
		<!-- and private page, configure this to FALSE	-->
	<value id="private">FALSE</value>
	<!-- If private part, specify minimum right	-->
	<value id="RequiredRight">0,1,2,3,4,5,6</value>
	<!-- Force visitor to use HTTPS connexion	-->
	<value id="requireSSL">FALSE</value>
	<!-- Name for session of this pool		-->
	<value id="SessName">ZUNOSXA</value>
	<!-- DBPool to use for store session		-->
	<value id="SessDBPool" modifRight="0">1</value>
	<!-- Timeout of the session			-->
	<value id="SessTimeOut">32400</value>
	<!-- Lenght of the session ID			-->
	<value id="SessKeyLenght">32</value>
	<!-- Frameset template directory		-->
	<value id="FramesetTmpDir" modifRight="0">FramesetNormal/</value>
	<!-- Display Navigation bar			-->
	<value id="DisplayNavBar">FALSE</value>
	<!-- Right to acces page modif and autoring	-->
	<value id="PageManageRequiredRight" modifRight="0">0,1,2,3</value>
	<!-- CSS theme used for graphical display	-->
	<value id="CSSTheme">defaut</value>
	<!-- CSS theme used for graphical display	-->
	<value id="isOnMenu">false</value>
    </group>
