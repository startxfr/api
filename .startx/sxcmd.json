{
    "version": "0.2",
    "main_cmd": "~/.sxcmd/bin/sxcmd",
    "local_cmd": ".startx/sxcmd",
    "output_prefix": "[sxcmd-api]",
    "project": {
        "version": "0.3",
        "name": "sxapi"
    },
    "git": {
        "branch_dev": "dev",
        "branch_testing": "testing",
        "branch_production": "production"
    },
    "docgenerator": {
        "docpath": "/var/www/html/startx/api.wiki/",
        "default_commit_msg": "generation de la documentation"
    },
    "datatbases": {
        "nosql": {
            "host": "localhost",
            "user": "dev",
            "pwd": "dev",
            "database": "sxapi",
            "dump_dir": ".startx/db-dump",
            "exclude_table": "logs,sxapi.logs,startx.logs,sxapi.session,cea.logs,cea.session"
        }
    }
}
